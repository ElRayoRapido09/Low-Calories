{
    "builds": [
        {
            "src": "lowcalories/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "lowcalories/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "lowcalories.settings"
    }
}